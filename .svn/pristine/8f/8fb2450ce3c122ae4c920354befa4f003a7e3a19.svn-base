	//数据处理批量删除
	@RequestMapping(value = "/admindataprocessbatchdelete", method = { RequestMethod.POST }, produces = "application/json; charset=utf-8")
	@ResponseBody
	private Map admindataprocessbatchdelete(HttpServletRequest request,String str) throws IOException {
		Map map = new HashMap<String, String>();
		try{
			String[] strid = str.split("&");
			for(int i=0;i<strid.length;i++){
				dt_dataprocessservice.delete(Integer.valueOf(strid[i]));
			}
			map.put("code", "100");
		}catch(Exception e){
			e.printStackTrace();
			map.put("code", "400");
		}
		return map;
	}	
