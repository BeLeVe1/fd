	@RequestMapping(value = "/admindatarecordaddajax", method = { RequestMethod.POST }, produces = "application/json; charset=utf-8")
	@ResponseBody
	private Map admindatarecordaddajax(HttpServletRequest request,Integer dataprocess_id,String time,
			String text,Integer number) throws IOException {
		HttpSession session = request.getSession();
		Map map = new HashMap<String, String>();
		try{
			//获取登录用户信息
			dt_admin admin = (dt_admin) session.getAttribute("admin");
			//时间转换成时间戳
			SimpleDateFormat sdf = new SimpleDateFormat("yyyy-MM-dd");
			//SimpleDateFormat sdf2 = new SimpleDateFormat("yyyy-MM-dd HH:mm:ss");
			Date date = sdf.parse(time);
			long recordtime = date.getTime()/1000;
			//保存记录
			dt_datarecord datarecord = new dt_datarecord();
			datarecord.setDataprocess_id(dataprocess_id);
			datarecord.setText(text);
			datarecord.setNumber(number);
			datarecord.setRecordtime(Integer.valueOf(String.valueOf(recordtime)));
			datarecord.setAdmin_id(admin.getAdmin_id());
			datarecord.setCreatetime(Integer.valueOf(String.valueOf(System.currentTimeMillis() / 1000)));
			dt_datarecordservice.saveOrUpdate(datarecord);
			
			//获取当年日期
			Calendar calendar = Calendar.getInstance();
	        calendar.set(Calendar.MONTH,0);
	        calendar.set(Calendar.DAY_OF_MONTH,1);
	        calendar.set(Calendar.HOUR_OF_DAY,0);
	        calendar.set(Calendar.MINUTE,0);
	        calendar.set(Calendar.SECOND,0);
	        calendar.set(Calendar.MILLISECOND,0);
	        Long time1 = calendar.getTime().getTime()/1000;//1年
	        
	        calendar.add(Calendar.MONTH,12);
	        Long time0 = calendar.getTime().getTime()/1000;//明年
	        
	        calendar.add(Calendar.MONTH,-24);
	        Long time2 = calendar.getTime().getTime()/1000;//2年
	        
	        calendar.add(Calendar.MONTH,-12);
	        Long time3 = calendar.getTime().getTime()/1000;//3年
	        
	        calendar.add(Calendar.MONTH,-12);
	        Long time4 = calendar.getTime().getTime()/1000;//4年
	        
	        calendar.add(Calendar.MONTH,-12);
	        Long time5 = calendar.getTime().getTime()/1000;//5年
	        
	        calendar.add(Calendar.MONTH,-12);
	        Long time6 = calendar.getTime().getTime()/1000;//6年
	        
	        calendar.add(Calendar.MONTH,-12);
	        Long time7 = calendar.getTime().getTime()/1000;//7年
	        
	        calendar.add(Calendar.MONTH,-12);
	        Long time8 = calendar.getTime().getTime()/1000;//8年
	        
	        calendar.add(Calendar.MONTH,-12);
	        Long time9 = calendar.getTime().getTime()/1000;//9年
	        
	        calendar.add(Calendar.MONTH,-12);
	        Long time10 = calendar.getTime().getTime()/1000;//10年
	        
	        dt_dataprocess  dataprocess = dt_dataprocessservice.get(dataprocess_id);
	        if(recordtime>=time1&&recordtime<time0) {
	        	dataprocess.setAge1(dataprocess.getAge1()+number);
	        }else if(recordtime>=time2&&recordtime<time1) {
	        	dataprocess.setAge2(dataprocess.getAge2()+number);
	        }else if(recordtime>=time3&&recordtime<time2) {
	        	dataprocess.setAge3(dataprocess.getAge3()+number);
	        }else if(recordtime>=time4&&recordtime<time3) {
	        	dataprocess.setAge4(dataprocess.getAge4()+number);
	        }else if(recordtime>=time5&&recordtime<time4) {
	        	dataprocess.setAge5(dataprocess.getAge5()+number);
	        }else if(recordtime>=time6&&recordtime<time5) {
	        	dataprocess.setAge6(dataprocess.getAge6()+number);
	        }else if(recordtime>=time7&&recordtime<time6) {
	        	dataprocess.setAge7(dataprocess.getAge7()+number);
	        }else if(recordtime>=time8&&recordtime<time7) {
	        	dataprocess.setAge8(dataprocess.getAge8()+number);
	        }else if(recordtime>=time9&&recordtime<time8) {
	        	dataprocess.setAge9(dataprocess.getAge9()+number);
	        }else if(recordtime>=time10&&recordtime<time9) {
	        	dataprocess.setAge10(dataprocess.getAge10()+number);
	        }
	        dt_dataprocessservice.saveOrUpdate(dataprocess);
	        
			map.put("code", "100");
			map.put("info", "添加成功");
		}catch(Exception e){
			e.printStackTrace();
			map.put("code", "400");
			map.put("info", "添加失败");
		}
		return map;
	}
