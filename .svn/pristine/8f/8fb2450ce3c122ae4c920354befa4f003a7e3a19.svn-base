        calendar.add(Calendar.MONTH,-12);
        Long time9 = calendar.getTime().getTime()/1000;//9年
        
        calendar.add(Calendar.MONTH,-12);
        Long time10 = calendar.getTime().getTime()/1000;//10年
		
		Long starttime = (long) 0;
		Long endtime = (long) 0;
		
		if(type==1) {
			starttime = time1;
